{
    "version": 2,
    "rewrites": [
      {
        "source": "/public/(.*)",
        "destination": "/public/$1"
      },
      {
        "source": "/(.*\\.(png|jpg|jpeg|gif|svg|ico))",
        "destination": "/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  }